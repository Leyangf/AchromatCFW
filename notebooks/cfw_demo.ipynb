  "source": [
    "focal_shift_curve = _load_defocus()  # Load defocus data from file\n",
    "wl = focal_shift_curve[:, 0]  # Load wavelength data\n",
    "CHLdata  = focal_shift_curve[:, 1] # Load focal shift data\n",
    "\n",
    "prods = channel_products()\n",
    "sensor_map = {\n",
    "    \"R\": prods[\"red\"][:, 1],\n",
    "    \"G\": prods[\"green\"][:, 1],\n",
    "    \"B\": prods[\"blue\"][:, 1],\n",
    "}"
  ]
}
